{
  "name": "google-place-scraper",
  "version": "1.1.0",
  "description": "A desktop application to scrap Google Place or Business from Google Map search results and export to excel file.",
  "main": "index.js",
  "scripts": {
    "babel_build": "babel --source-maps --out-dir=babel-output src",
    "copy-babel-output": "node import-babel-scripts.js",
    "start-cli": "npm run babel_build && node ./babel-output/index.js",
    "start-pm2": "npm run babel_build && pm2 start ecosystem.config.js --time",
    "start-review": "npm run babel_build && node ./babel-output/review.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "npm run babel_build & npm run copy-babel-output & build"
  },
  "author": "Hendro Wibowo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.4",
    "date.js": "^0.3.3",
    "delay": "^5.0.0",
    "jquery": "^3.6.0",
    "mariadb": "^2.5.5",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "puppeteer": "^12.0.1",
    "puppeteer-core": "^10.2.0",
    "sequelize": "^6.12.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^7.5.0"
  },
  "build": {
    "appId": "HendroWibowo.GooglePlaceScraper",
    "productName": "Google Place Scraper",
    "copyright": "Hendro Wibowo",
    "directories": {
      "app": ".",
      "output": "build",
      "buildResources": "assets"
    },
    "files": [
      "package.json",
      "./**/*",
      "node_modules"
    ],
    "nsis": {
      "installerIcon": "assets/images/favicon.ico",
      "uninstallerIcon": "assets/images/favicon.ico",
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "oneClick": false
    },
    "win": {
      "icon": "assets/images/ms-icon-310x310.png",
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    }
  }
}
